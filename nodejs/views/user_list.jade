doctype html
html
  head
    title("Godai Quest User List")
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    h1 GodaiQuest
      span#small 　　　新感覚 情報共有 SNS
    each dic in userinfo.uesr_dic
      if dic
        if dic.auser
          h2
            A(href="info_list?view_id="+dic.auser.user_id) 
              img(src=dic.auser.uri_image, style="width:60px")
              span #{dic.auser.user_name}さん
          p #{mapUserItem[dic.auser.user_id].length}個の未読情報があります
    if error_message
       div
         p.error_message=error_message
